{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "watch",
      "group": "build",
      "isBackground": true,
      "problemMatcher": ["$ts-webpack-watch"]
    },
    {
      "label": "delay",
      "type": "shell",
      "command": "sleep 5",
      "windows": {
        "command": "ping 127.0.0.1 -n 5 > nul"
      },
      "group": "none",
      "presentation": {
        "reveal": "silent",
        "panel": "new"
      }
    }
  ]
}
